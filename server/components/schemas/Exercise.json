{
  "components": {
    "schemas": {
      "Exercise": {
        "type": "object",
        "required": [
          "title"
        ],
        "properties": {
          "title": {
            "type": "string",
            "description": "The title of the exercise."
          },
          "questions": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "List of questions related to the exercise."
          },
          "createdAt": {
            "type": "string",
            "format": "date-time",
            "description": "Timestamp of exercise creation."
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time",
            "description": "Timestamp of the last update to the exercise."
          }
        }
      }
    }
  }
}
